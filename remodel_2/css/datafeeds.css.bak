@charset "utf-8";
/* CSS Document */

div#tick {
  z-index: 0;
  margin: 0px 0px 12px 15px;
  padding: 0;
  width: 145px;
  height: 78px;
}


#home_text {
	margin-top: 0px;
	padding-right: 10px;
	text-align:justify;
}
/****************************************************/

#home_right_pic {
	margin:2px 0px 0px 0px;;
	padding:0px 6px 0px 0px;
	text-align: right;

}

#home_right_pic img {
	margin:0px 0px 0px 5px;;
	padding: 0px 0px 0px 2px;
	text-align: center;
}

#home_right_pic_text {
	margin: 0px;
	padding: 0px 0px 0px 0px;
	text-align: center;

		
}


#home_right_pic_text a:link {text-decoration: none; color:#666666;}  
#home_right_pic_text a:visited {text-decoration: none; color:#666666; }
#home_right_pic_text a:hover {text-decoration: none; color:#000000;}
#home_right_pic_text a:active {text-decoration: none; color:#666666;}

